<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.acdat.springboot.demo.modelo.EntidadClientesDestinos" table="clientes_destinos" schema="public"
           catalog="vuelos">
        <composite-id mapped="true" class="com.acdat.springboot.demo.modelo.EntidadClientesDestinosPK">
            <key-property name="clienteId">
                <column name="cliente_id" sql-type="integer"/>
            </key-property>
            <key-property name="destinoId">
                <column name="destino_id" sql-type="integer"/>
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>